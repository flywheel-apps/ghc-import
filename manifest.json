{
	"name": "ghc-import",
	"label": "GHC import",
	"description": "Import dicoms from Google Healthcare API",
	"version": "0.5.1",
	"flywheel": "0",
	"inputs": {
		"import_ids": {
			"base": "file",
			"description": "JSON file with DICOM Series Instance or Study Instance UIDs, HL7 Message IDs , FHIR Resource References. Schema: {'dicoms': ['1.2.840.113619.2.243.4231785106118302.10626.7104.1396227'], 'fhirs': ['Patient/d879d684-892a-4ba7-c4c0-9b68c481635f', 'Encounter/43dc1f3a-049b-49d0-b67e-21cd64acb1d9'], 'hl7s': ['sXiWf0k3rtURTkhi7144lsgfWgbP41OG-3fv5zvjLtM=']}",
			"type": {
				"enum": [
					"json"
				]
			}
		},
		"key": {
			"base": "api-key"
		}
	},
	"config": {
		"auth_token_id": {
			"description": "GCP auth token id",
			"type": "string"
		},
		"de_identify": {
			"default": false,
			"description": "De-identify DICOMs before import",
			"type": "boolean"
		},
		"hc_dicomstore": {
			"default": "",
			"description": "Healthcare API dicom store",
			"type": "string"
		},
		"hc_fhirstore": {
			"default": "",
			"description": "Healthcare API FHIR store",
			"type": "string"
		},
		"hc_hl7store": {
			"default": "",
			"description": "Healthcare API HL7 store",
			"type": "string"
		},
		"log_level": {
			"default": "INFO",
			"description": "Log verbosity level (ERROR|WARNING|INFO|DEBUG)",
			"type": "string"
		},
		"project_id": {
			"description": "Destination Flywheel project",
			"type": "string"
		}
	},
	"command": "./run.py",
	"author": "David Farkas",
	"maintainer": "David Farkas",
	"license": "Other",
	"source": "https://github.com/flywheel-apps/ghc-import",
	"url": "https://github.com/flywheel-apps/ghc-import",
	"custom": {
		"gear-builder": {
			"category": "importer",
			"container": "24b35b94a775be7db120749940ea7a57bd17bec5f70dde55ff0cd6953c732159",
			"image": "gear-builder-rsnkwdikmg-20190625121434"
		}
	}
}
