{
	"name": "ghc-import",
	"label": "GHC import",
	"description": "Import dicoms from Google Healthcare API",
	"version": "0.4.21",
	"flywheel": "0",
	"inputs": {
		"key": {
			"base": "api-key"
		}
	},
	"config": {
		"auth_token_id": {
			"description": "GCP auth token id",
			"type": "string"
		},
		"hc_dicomstore": {
			"description": "Healthcare API dicom store",
			"type": "string",
			"optional": true
		},
		"hc_hl7store": {
			"description": "Healthcare API HL7 store",
			"type": "string",
			"optional": true
		},
		"hc_fhirstore": {
			"description": "Healthcare API FHIR store",
			"type": "string",
			"optional": true
		},
		"uids": {
			"description": "List of Study/SeriesInstanceUIDs to import",
			"items": {
				"type": "string"
			},
			"type": "array",
			"default": []
		},
		"hl7_msg_ids": {
			"description": "HL7 message IDs to import",
			"items": {
				"type": "string"
			},
			"type": "array",
			"default": []
		},
		"fhir_resource_refs": {
			"description": "FHIR resource references resource_type/id",
			"items": {
				"type": "string"
			},
			"type": "array",
			"default": []
		},
		"de_identify": {
			"description": "De-identify DICOMs before import",
			"type": "boolean",
			"default": false
		},
		"project_id": {
			"description": "Destination Flywheel project",
			"type": "string"
		},
		"log_level": {
			"description": "Log verbosity level (ERROR|WARNING|INFO|DEBUG)",
			"type": "string",
			"default": "INFO"
		}
	},
	"author": "David Farkas",
	"maintainer": "David Farkas",
	"license": "Other",
	"source": "https://github.com/flywheel-apps/ghc-import",
	"url": "https://github.com/flywheel-apps/ghc-import",
	"command": "python run.py",
	"custom": {
		"gear-builder": {
			"image": "ghc-import"
		}
	}
}
