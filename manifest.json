{
	"name": "ghc-import",
	"label": "GHC import",
	"description": "Import dicom files from Google Healthcare API",
	"version": "0.2",
	"flywheel": "0",
	"inputs": {
		"key": {
			"base": "api-key"
		}
	},
	"config": {
		"uids": {
			"description": "Import studies/series with these uids",
			"items": {
				"type": "string"
			},
			"type": "array"
		},
		"level": {
			"description": "Import level studies/series",
			"type": "string",
			"default": "series"
		},
		"project": {
			"description": "Google project id",
			"type": "string"
		},
		"location": {
			"description": "Data location eg. us-central1",
			"type": "string"
		},
		"dataset": {
			"description": "Google Healtcare dataset",
			"type": "string"
		},
		"dicomstore": {
			"description": "Google Healtcare Dicom store",
			"type": "string"
		},
		"log-level": {
			"description": "Log level, the usual DEBUG, WARNING, INFO etc.",
			"type": "string",
			"default": "INFO"
		}
	},
	"author": "David Farkas",
	"maintainer": "David Farkas",
	"license": "Other",
	"source": "https://github.com/flywheel-apps/ghc-import",
	"url": "https://github.com/flywheel-apps/ghc-import",
	"custom": {
		"gear-builder": {
			"image": "ghc-import"
		}
	}
}