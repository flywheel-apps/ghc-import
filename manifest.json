{
	"name": "ghc-import",
	"label": "GHC import",
	"description": "Import dicom files from Google Healthcare API",
	"version": "0.2.8",
	"flywheel": "0",
	"inputs": {
		"key": {
			"base": "api-key"
		}
	},
	"config": {
		"dicomweb_uri": {
			"description": "DICOMWeb API URL",
			"type": "string"
		},
		"uids": {
			"description": "List of Study/Series UIDs to import",
			"items": {
				"type": "string"
			},
			"type": "array"
		},
		"uid_field": {
			"description": "UID field to search on (Study/SeriesInstanceUID)",
			"type": "string",
			"default": "SeriesInstanceUID"
		},
		"de_identify": {
			"description": "De-identify dicoms before import",
			"type": "boolean",
			"default": false
		},
		"group_id": {
			"description": "Flywheel group id to import into",
			"type": "string"
		},
		"project_label": {
			"description": "Flywheel project label to import into",
			"type": "string"
		},
		"log_level": {
			"description": "Log verbosity level (ERROR|WARNING|INFO|DEBUG)",
			"type": "string",
			"default": "INFO"
		}
	},
	"author": "David Farkas",
	"maintainer": "David Farkas",
	"license": "Other",
	"source": "https://github.com/flywheel-apps/ghc-import",
	"url": "https://github.com/flywheel-apps/ghc-import",
	"custom": {
		"gear-builder": {
			"image": "ghc-import"
		}
	}
}
